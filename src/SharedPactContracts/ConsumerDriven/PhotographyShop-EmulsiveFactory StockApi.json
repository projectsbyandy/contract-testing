{
  "consumer": {
    "name": "PhotographyShop"
  },
  "interactions": [
    {
      "description": "An opinion service request",
      "pending": false,
      "providerStates": [
        {
          "name": "an Stock request is made with a film name"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/Stock/Portra400"
      },
      "response": {
        "body": {
          "content": {
            "filmStock": {
              "film": {
                "filmType": 2,
                "name": "Fuji400H"
              },
              "stock": {
                "inStock": 100,
                "onOrder": 200
              }
            },
            "httpStatusCode": 200
          },
          "contentType": "application/json",
          "encoded": false
        },
        "headers": {
          "Content-Type": [
            "application/json; charset=utf-8"
          ]
        },
        "matchingRules": {
          "body": {
            "$.filmStock.film.filmType": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.filmStock.film.name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.filmStock.stock.inStock": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "number"
                }
              ]
            },
            "$.filmStock.stock.onOrder": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "number"
                }
              ]
            },
            "$.httpStatusCode": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {}
        },
        "status": 200
      },
      "type": "Synchronous/HTTP"
    }
  ],
  "metadata": {
    "pactRust": {
      "ffi": "0.4.5",
      "models": "1.1.2"
    },
    "pactSpecification": {
      "version": "4.0"
    }
  },
  "provider": {
    "name": "EmulsiveFactory StockApi"
  }
}